{
  "name": "unwrap-or",
  "version": "0.1.1-alpha",
  "license": "ISC",
  "description": "A TypeScript implementation of Rust's Option and Result types.",
  "keywords": [
    "TypeScript",
    "Rust",
    "Option",
    "Some",
    "None",
    "Result",
    "Ok",
    "Err",
    "monad",
    "ADT",
    "or"
  ],
  "author": {
    "name": "Roman Hnatiuk",
    "email": "hnatiukr@pm.me",
    "url": "https://github.com/hnatiukr"
  },
  "repository": {
    "url": "git+https://github.com/hnatiukr/unwrap-or.git"
  },
  "homepage": "https://github.com/hnatiukr/unwrap-or/",
  "scripts": {
    "test": "vitest run",
    "fmt": "prettier --write .",
    "check:fmt": "prettier --check .",
    "check:types": "tsc",
    "check:all": "pnpm run check:fmt && pnpm run check:types",
    "prebuild": "rimraf lib",
    "build:dts": "copyfiles --up 1 --all \"./src/**/*.d.ts\" lib",
    "build:js": "tsc --build tsconfig.build.json",
    "postbuild:js": "tsc-alias -p tsconfig.build.json",
    "build": "pnpm run build:dts && pnpm run build:js"
  },
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "copyfiles": "2.4.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tsc-alias": "1.8.15",
    "typescript": "5.8.3",
    "vitest": "3.1.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
