{
  "name": "unwrap-or",
  "version": "0.1.4-beta",
  "license": "MIT",
  "description": "A TypeScript implementation of Rust's Option and Result types.",
  "author": {
    "name": "Roman Hnatiuk",
    "email": "hnatiukr@pm.me",
    "url": "https://github.com/hnatiukr"
  },
  "repository": {
    "url": "https://github.com/hnatiukr/unwrap-or",
    "type": "git"
  },
  "homepage": "https://hnatiukr.github.io/unwrap-or",
  "keywords": [
    "TypeScript",
    "Rust",
    "Option",
    "Some",
    "None",
    "Result",
    "Ok",
    "Err",
    "monad",
    "ADT",
    "or"
  ],
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./option": {
      "types": "./lib/option.d.ts",
      "import": "./lib/option.js",
      "require": "./lib/option.js"
    },
    "./result": {
      "types": "./lib/result.d.ts",
      "import": "./lib/result.js",
      "require": "./lib/result.js"
    }
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "copyfiles": "2.4.1",
    "prettier": "3.5.3",
    "terser": "5.43.1",
    "typescript": "5.8.3",
    "vitest": "3.1.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
