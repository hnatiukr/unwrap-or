{
  "name": "@lab/unwrap-or",
  "version": "1.0.8",
  "license": "MIT",
  "description": "A TypeScript implementation of Rust's Option and Result types.",
  "homepage": "https://codeberg.org/hnatiukr/unwrap-or",
  "keywords": [
    "TypeScript",
    "Rust",
    "Option",
    "Some",
    "None",
    "Result",
    "Ok",
    "Err",
    "monad",
    "ADT",
    "or"
  ],
  "exports": {
    "./option": "./lib/option/option.ts",
    "./result": "./lib/result/result.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13"
  },
  "tasks": {
    "check:all": "deno fmt --check && deno lint && deno check && deno test",
    "prepublish": "deno task check:all && deno publish --dry-run --allow-dirty",
    "publish": "deno task check:all && deno publish"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
